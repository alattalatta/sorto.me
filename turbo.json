{
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"]
    },
    "prebuild": {
      "dependsOn": ["^build"],
      "outputs": ["out/**", "public/rss.xml", "public/sitemap.md.xml"]
    },
    "@app/main#build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"]
    },
    "@lib/mdx#build": {
      "dependsOn": ["^build"],
      "outputs": ["compiler/index.js"]
    },
    "dev": {
      "cache": false
    },
    "test": {},
    "lint": {}
  }
}
