{
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"]
    },
    "@app/web#build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "public/rss.xml", "public/sitemap.md.xml", "public/gen/**"]
    },
    "@lib/mdx#build": {
      "dependsOn": ["^build"],
      "outputs": ["compiler/index.js"]
    },
    "dev": {
      "cache": false,
      "dependsOn": ["@contents/docs#build", "@contents/posts#build"]
    },
    "test": {}
  }
}
