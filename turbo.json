{
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "data/**"]
    },
    "dev": {
      "cache": false,
      "dependsOn": ["@app/docs#build", "@app/posts#build"]
    },
    "watch": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "test": {}
  }
}
