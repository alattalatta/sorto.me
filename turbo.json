{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "typecheck",
        "^build"
      ],
      "outputs": [
        "dist"
      ]
    },
    "dev": {
      "dependsOn": [
        "^build"
      ],
      "persistent": true
    },
    "typecheck": {}
  }
}