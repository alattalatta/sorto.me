{
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"]
    },
    "@app/web#build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "out", "public/rss.xml", "public/sitemap.md.xml"]
    },
    "@lib/mdx#build": {
      "dependsOn": ["^build"],
      "outputs": ["compiler/index.js"]
    },
    "dev": {
      "cache": false
    },
    "test": {}
  }
}
