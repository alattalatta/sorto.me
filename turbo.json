{
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "data/**"]
    },
    "@lib/mdx#build": {
      "dependsOn": ["^build"],
      "outputs": ["compiler/index.js"]
    },
    "dev": {
      "cache": false,
      "dependsOn": ["@contents/docs#build", "@contents/posts#build"]
    },
    "test": {}
  }
}
